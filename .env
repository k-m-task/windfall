# Environment variables for Windfall backend application

# MySQL Database Configuration (if needed)
# These values are already set in application-mysql.properties, but can be overridden here
# MYSQL_HOST=localhost
# MYSQL_PORT=3306
# MYSQL_DATABASE=windfall
# MYSQL_USER=windfall
# MYSQL_PASSWORD=windfall

# Application Configuration
# SERVER_PORT=8080

# Logging Configuration
# LOGGING_LEVEL_ROOT=INFO
# LOGGING_LEVEL_COM_WINDFALL=DEBUG

# Security Configuration (if needed)
# JWT_SECRET=your-secret-key
# JWT_EXPIRATION_MS=86400000

# These environment variables can be used to override properties in application.properties
# or application-mysql.properties when running the application
